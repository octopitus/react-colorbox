    "react": "^0.13.3",
    "reactcss": "^0.3.2"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "concurrently": "^0.1.1",
    "react-hot-loader": "^1.2.9",
    "react-map-styles": "^0.2.0",
